		</dependency>
		<dependency>
    		<groupId>org.scala-lang</groupId>
    		<artifactId>scala-swing</artifactId>
    		<version>2.10.5</version>
		</dependency>		
<dependency>
    <groupId>com.dongxiguo</groupId>
    <artifactId>zero-log_2.11</artifactId>
    <version>0.3.6</version>
</dependency>
<dependency>
    <groupId>com.dongxiguo</groupId>
    <artifactId>fastring_2.11</artifactId>
    <version>0.2.4</version>
</dependency>
	<dependency>
    <groupId>net.sourceforge.owlapi</groupId>
    <artifactId>owlapi-distribution</artifactId>
    <version>4.1.4</version>
</dependency>
<dependency>
    <groupId>org.semanticweb</groupId>
    <artifactId>HermiT</artifactId>
    <version>1.3.6</version>
    <exclusions>
        <exclusion> 
         	<groupId>owlapi</groupId>
  			<artifactId>owlapi</artifactId>  			
        </exclusion>
      </exclusions> 
</dependency>

<dependency>
    <groupId>net.sourceforge.owlapi</groupId>
    <artifactId>owlapi-distribution</artifactId>
    <version>3.5.2</version>
</dependency>
	<!-- Dependencies for Fame -->
	<dependency>
        <groupId>fame.com</groupId>
        <artifactId>fame</artifactId>
        <version>1.0</version>
    </dependency>
    <dependency>
   		 <groupId>com.google.guava</groupId>
    	<artifactId>guava</artifactId>
    	<version>21.0</version>
	</dependency>
	<dependency>
    	<groupId>com.google.inject</groupId>
    	<artifactId>guice</artifactId>
    	<version>4.1.0</version>
	</dependency>
	<dependency>
    	<groupId>org.jvnet.substance</groupId>
    	<artifactId>substance</artifactId>
    	<version>4.3</version>
	</dependency>
	<dependency>
    <groupId>org.apache.commons</groupId>
    <artifactId>commons-lang3</artifactId>
    <version>3.6</version>
</dependency>
</dependencies>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.7.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>3.0.0</version>
        <configuration>
          <warSourceDirectory>WebContent</warSourceDirectory>
        </configuration>
      </plugin>
    </plugins>
    <finalName>${project.artifactId}</finalName> <!-- added to remove Version from WAR file -->
  </build>
  <repositories>
      <repository>
         <id>BBOP</id>
         <url>http://code.berkeleybop.org/maven/repository/</url>
      </repository>
      <repository>
         <id>Jspresso</id>
         <url>http://repository.jspresso.org/maven2/</url>
      </repository>
   </repositories>
</project>